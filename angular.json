{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "carousel-simple",
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "projects": {
    "carousel-simple": {
      "projectType": "library",
      "root": "libs/carousel-simple",
      "sourceRoot": "libs/carousel-simple/src",
      "prefix": "ngxd",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/carousel-simple/tsconfig.lib.json",
            "project": "libs/carousel-simple/ng-package.json",
            "buildableProjectDepsInPackageJsonType": "dependencies"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/carousel-simple/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/carousel-simple/src/**/*.ts",
              "libs/carousel-simple/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/carousel-simple/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/carousel-simple"
          ]
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "carousel-sample": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/carousel-sample",
      "sourceRoot": "apps/carousel-sample/src",
      "prefix": "ngxd-sample",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/carousel-sample",
            "index": "apps/carousel-sample/src/index.html",
            "main": "apps/carousel-sample/src/main.ts",
            "polyfills": "apps/carousel-sample/src/polyfills.ts",
            "tsConfig": "apps/carousel-sample/tsconfig.app.json",
            "assets": [
              "apps/carousel-sample/src/favicon.ico",
              "apps/carousel-sample/src/assets"
            ],
            "styles": [
              "apps/carousel-sample/src/styles.scss"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/carousel-sample/src/environments/environment.ts",
                  "with": "apps/carousel-sample/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "outputs": [
            "{options.outputPath}"
          ]
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "carousel-sample:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "carousel-sample:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "carousel-sample:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/carousel-sample/src/**/*.ts",
              "apps/carousel-sample/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/carousel-sample/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/apps/carousel-sample"
          ]
        }
      }
    },
    "carousel-sample-e2e": {
      "root": "apps/carousel-sample-e2e",
      "sourceRoot": "apps/carousel-sample-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/carousel-sample-e2e/cypress.json",
            "tsConfig": "apps/carousel-sample-e2e/tsconfig.e2e.json",
            "devServerTarget": "carousel-sample:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "carousel-sample:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/carousel-sample-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  }
}